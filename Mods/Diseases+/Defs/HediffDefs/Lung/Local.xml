<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<HediffDef ParentName="InfectionBase">
		<defName>Diseases_HediffDef_PulmonaryEdema</defName>
		<label>Pulmonary edema</label>
		<description>Accumulation of fluid in the lung area caused by disease.</description>
		<initialSeverity>1.0</initialSeverity>
		<maxSeverity>1.0</maxSeverity>
		<tendable>false</tendable>
		<makesSickThought>true</makesSickThought>
		<cureAllAtOnceIfCuredByItem>true</cureAllAtOnceIfCuredByItem>
		<comps>
			<li Class="HediffCompProperties_Disappears">
				<disappearsAfterTicks>30000~50000</disappearsAfterTicks>
				<showRemainingTime>false</showRemainingTime>
			</li>
		</comps>
		<stages>
			<li>
				<painOffset>0.05</painOffset>
				<partEfficiencyOffset>-0.2</partEfficiencyOffset>
			</li>
		</stages>
	</HediffDef>
	<HediffDef ParentName="InfectionBase">
		<defName>Diseases_HediffDef_Angina</defName>
		<label>Angina</label>
		<description>Sore throat making speech difficult.</description>
		<initialSeverity>1.0</initialSeverity>
		<maxSeverity>1.0</maxSeverity>
		<tendable>true</tendable>
		<makesSickThought>true</makesSickThought>
		<comps>
			<li Class="HediffCompProperties_TendDuration">
				<baseTendDurationHours>24</baseTendDurationHours>
				<disappearsAtTotalTendQuality>2</disappearsAtTotalTendQuality>
			</li>
		</comps>
		<stages>
			<li>
				<painOffset>0.15</painOffset>
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<setMax>0.1</setMax>
					</li>
				</capMods>
			</li>
		</stages>
	</HediffDef>
	<HediffDef ParentName="InfectionBase">
		<defName>Diseases_HediffDef_Pneumothorax</defName>
		<label>Pneumothorax</label>
		<description>A disease that often occurs as a consequence of respiratory diseases. It also occurs in healthy people.</description>
		<descriptionHyperlinks>
			<HediffDef>Diseases_HediffDef_PulmonaryEdema</HediffDef>
		</descriptionHyperlinks>
		<initialSeverity>0.1</initialSeverity>
		<lethalSeverity>1.0</lethalSeverity>
		<maxSeverity>1.0</maxSeverity>
		<tendable>true</tendable>
		<makesSickThought>true</makesSickThought>
		<comps>
			<li Class="HediffCompProperties_Immunizable">
				<severityPerDayNotImmune>0.8</severityPerDayNotImmune>
			</li>
			<li Class="HediffCompProperties_TendDuration">
				<baseTendDurationHours>24</baseTendDurationHours>
				<severityPerDayTended>-1.5</severityPerDayTended>
			</li>
			<li Class="Diseases.Hediffs.HediffCompProperties_Storage">
				<comps>
					<li>
						<compClass>Diseases.Hediffs.HediffComp_DiseaseAfterTicks</compClass>
						<intervalTicks>150000~200000</intervalTicks>
						<hediffDefs>
							<li>Diseases_HediffDef_PulmonaryEdema</li>
						</hediffDefs>
						<partsToAffect>
							<li>Lung</li>
						</partsToAffect>
						<countToAffect>2</countToAffect>
						<letter>false</letter>
						<chance>0.5</chance>
						<severityRange>0.2~1.0</severityRange>
					</li>
				</comps>
			</li>
		</comps>
		<stages>
			<li>
				<label>easy</label>
				<painOffset>0.05</painOffset>
				<capMods>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.1</offset>
					</li>
				</capMods>
			</li>
			<li>
				<label>medium</label>
				<minSeverity>0.5</minSeverity>
				<painOffset>0.1</painOffset>
				<restFallFactorOffset>0.1</restFallFactorOffset>
				<capMods>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.2</offset>
					</li>
				</capMods>
			</li>
			<li>
				<label>severe</label>
				<minSeverity>0.8</minSeverity>
				<painOffset>0.2</painOffset>
				<restFallFactorOffset>0.25</restFallFactorOffset>
				<lifeThreatening>true</lifeThreatening>
				<capMods>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.3</offset>
					</li>
				</capMods>
			</li>
		</stages>
	</HediffDef>
	<HediffDef ParentName="InfectionBase">
		<defName>Diseases_HediffDef_Pneumonia</defName>
		<label>Pneumonia</label>
		<description>Severe lung disease. Can cause many treatment complications.</description>
		<descriptionHyperlinks>
			<HediffDef>Diseases_HediffDef_PulmonaryEdema</HediffDef>
			<HediffDef>Diseases_HediffDef_Pneumothorax</HediffDef>
			<HediffDef>Diseases_HediffDef_Fever</HediffDef>
		</descriptionHyperlinks>
		<makesSickThought>true</makesSickThought>
		<lethalSeverity>1</lethalSeverity>
		<tendable>true</tendable>
		<comps>
			<li Class="HediffCompProperties_TendDuration">
				<baseTendDurationHours>8</baseTendDurationHours>
				<severityPerDayTended>-0.1</severityPerDayTended>
			</li>
			<li Class="HediffCompProperties_Immunizable">
				<severityPerDayNotImmune>0.28</severityPerDayNotImmune>
				<immunityPerDaySick>0.25</immunityPerDaySick>
				<severityPerDayImmune>-0.2</severityPerDayImmune>
				<immunityPerDayNotSick>-0.06</immunityPerDayNotSick>
			</li>
			<li Class="Diseases.Hediffs.HediffCompProperties_Storage">
				<comps>
					<li>
						<compClass>Diseases.Hediffs.HediffComp_DiseaseAfterTicks</compClass>
						<intervalTicks>72000~150000</intervalTicks>
						<hediffDefs>
							<li>Diseases_HediffDef_PulmonaryEdema</li>
						</hediffDefs>
						<partsToAffect>
							<li>Lung</li>
						</partsToAffect>
						<countToAffect>2</countToAffect>
						<letter>false</letter>
						<chance>0.5</chance>
						<severityRange>0.2~1.0</severityRange>
					</li>
					<li>
						<compClass>Diseases.Hediffs.HediffComp_DiseaseAfterTicks</compClass>
						<intervalTicks>72000~150000</intervalTicks>
						<hediffDefs>
							<li>Diseases_HediffDef_Fever</li>
						</hediffDefs>
						<countToAffect>1</countToAffect>
						<letter>false</letter>
						<chance>0.5</chance>
						<severityRange>0.4~1.0</severityRange>
					</li>
					<li>
						<compClass>Diseases.Hediffs.HediffComp_DiseaseAfterTicks</compClass>
						<intervalTicks>72000~150000</intervalTicks>
						<hediffDefs>
							<li>Diseases_HediffDef_Pneumothorax</li>
						</hediffDefs>
						<partsToAffect>
							<li>Lung</li>
						</partsToAffect>
						<countToAffect>2</countToAffect>
						<letter>true</letter>
						<chance>0.25</chance>
						<severityRange>0.5~1.0</severityRange>
					</li>
				</comps>
			</li>
		</comps>
		<stages>
			<li>
				<label>minor</label>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.1</offset>
					</li>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.2</offset>
					</li>
				</capMods>
			</li>
			<li>
				<minSeverity>0.666</minSeverity>
				<label>major</label>
				<vomitMtbDays>1.5</vomitMtbDays>
				<lifeThreatening>true</lifeThreatening>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.2</offset>
					</li>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.3</offset>
					</li>
				</capMods>
			</li>
			<li>
				<minSeverity>0.833</minSeverity>
				<label>extreme</label>
				<lifeThreatening>true</lifeThreatening>
				<vomitMtbDays>2.75</vomitMtbDays>
				<painOffset>0.2</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.5</offset>
					</li>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.5</offset>
					</li>
				</capMods>
			</li>
		</stages>
	</HediffDef>
	<HediffDef ParentName="InfectionBase">
		<defName>Diseases_HediffDef_CommonCold</defName>
		<label>Common cold</label>
		<description>Mild respiratory tract infection.</description>
		<descriptionHyperlinks>
			<HediffDef>Diseases_HediffDef_PulmonaryEdema</HediffDef>
			<HediffDef>Diseases_HediffDef_Angina</HediffDef>
		</descriptionHyperlinks>
		<makesSickThought>true</makesSickThought>
		<lethalSeverity>1.0</lethalSeverity>
		<maxSeverity>1.0</maxSeverity>
		<initialSeverity>0.001</initialSeverity>
		<tendable>true</tendable>
		<comps>
			<li Class="HediffCompProperties_TendDuration">
				<baseTendDurationHours>24</baseTendDurationHours>
				<severityPerDayTended>-0.15</severityPerDayTended>
			</li>
			<li Class="HediffCompProperties_Immunizable">
				<severityPerDayNotImmune>0.35</severityPerDayNotImmune>
				<immunityPerDaySick>0.3</immunityPerDaySick>
				<severityPerDayImmune>-0.4</severityPerDayImmune>
				<immunityPerDayNotSick>-0.06</immunityPerDayNotSick>
			</li>
			<li Class="Diseases.Hediffs.HediffCompProperties_Storage">
				<comps>
					<li>
						<compClass>Diseases.Hediffs.HediffComp_DiseaseAfterTicks</compClass>
						<intervalTicks>72000~240000</intervalTicks>
						<hediffDefs>
							<li>Diseases_HediffDef_PulmonaryEdema</li>
						</hediffDefs>
						<partsToAffect>
							<li>Lung</li>
						</partsToAffect>
						<countToAffect>2</countToAffect>
						<letter>false</letter>
						<chance>0.5</chance>
						<severityRange>0.2~1.0</severityRange>
					</li>
					<li>
						<compClass>Diseases.Hediffs.HediffComp_DiseaseAfterTicks</compClass>
						<intervalTicks>40000~70000</intervalTicks>
						<hediffDefs>
							<li>Diseases_HediffDef_Fever</li>
						</hediffDefs>
						<countToAffect>1</countToAffect>
						<letter>false</letter>
						<chance>0.1</chance>
						<severityRange>0.85~1.0</severityRange>
					</li>
					<li>
						<compClass>Diseases.Hediffs.HediffComp_DiseaseAfterTicks</compClass>
						<intervalTicks>15000~30000</intervalTicks>
						<hediffDefs>
							<li>Diseases_HediffDef_Angina</li>
						</hediffDefs>
						<partsToAffect>
							<li>Neck</li>
						</partsToAffect>
						<countToAffect>1</countToAffect>
						<letter>true</letter>
						<chance>0.5</chance>
						<severityRange>0.75~1.0</severityRange>
					</li>
				</comps>
			</li>
		</comps>
		<stages>
			<li>
				<label>minor</label>
				<capMods>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.05</offset>
					</li>
				</capMods>
			</li>
			<li>
				<minSeverity>0.4</minSeverity>
				<label>medium</label>
				<vomitMtbDays>1.5</vomitMtbDays>
				<painOffset>0.05</painOffset>
				<capMods>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.1</offset>
					</li>
				</capMods>
			</li>
			<li>
				<minSeverity>0.6</minSeverity>
				<label>major</label>
				<vomitMtbDays>0.5</vomitMtbDays>
				<painOffset>0.10</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.05</offset>
					</li>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.15</offset>
					</li>
				</capMods>
			</li>
			<li>
				<minSeverity>0.8</minSeverity>
				<label>severe</label>
				<vomitMtbDays>0.75</vomitMtbDays>
				<painOffset>0.15</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.1</offset>
					</li>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.2</offset>
					</li>
				</capMods>
			</li>
		</stages>
	</HediffDef>
</Defs>