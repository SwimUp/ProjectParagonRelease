<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!-- 
        Astra Militarum Regimentum - Krieg (https://steamcommunity.com/sharedfiles/filedetails/?id=2890928741)

        2 Gasmasks.
    --> 
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>[1.4] Astra Militarum Regimentum - Krieg</li>
        </mods>
        <match Class="PatchOperationAdd">
            <xpath>Defs/Def[defName="FaceCoveringThick"]/apparel</xpath>
            <value>
                <li>ODZ_40k_IG_DKOK_Gasmask</li>
                <li>ODZ_40k_IG_DKOK_GrenadierGasmask</li>
            </value>
        </match>
    </Operation>

</Patch>
