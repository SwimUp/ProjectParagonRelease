<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!--
        [LF] Rimteska! (https://steamcommunity.com/sharedfiles/filedetails/?id=2854536595)

        3 Gas masks.
    -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>[LF] Rimteska!</li>
        </mods>
        <match Class="PatchOperationAdd">
            <xpath>Defs/Def[defName="FaceCoveringThick"]/apparel</xpath>
            <value>
                <li>RT_DraegerMask</li>
                <li>RT_JSGPM</li>
                <li>RT_Schutzmaske</li>
            </value>
        </match>
    </Operation>

</Patch>
