<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>*/ThingDef[defName="MedicineHerbal"]</xpath>
				<value>
					<ThingDef ParentName="MedicineBase">
						<defName>MedicineHerbal</defName>
						<label>herbal medicine</label>
						<description>A pack of herbal concoctions extracted from the healroot plant. It is less potent than industrial pharmaceuticals, but much better than nothing.</description>
						<graphicData>
							<texPath>Things/Item/Resource/Medicine/MedicineHerbal</texPath>
							<graphicClass>Graphic_StackCount</graphicClass>
							<drawSize>0.85</drawSize>
						</graphicData>
						<techLevel>Neolithic</techLevel>
						<stackLimit>50</stackLimit>
						<statBases>
							<MarketValue>3.5</MarketValue>
							<Mass>0.1</Mass>
							<Flammability>1.5</Flammability>
							<MedicalPotency>0.35</MedicalPotency>
							<MedicalQualityMax>0.50</MedicalQualityMax>
							<DeteriorationRate>6</DeteriorationRate>
							<MedicalTendMult>0.5</MedicalTendMult>
						</statBases>
						<tickerType>Rare</tickerType>
						<modExtensions>
							<li Class="ParagonWiki.Other.DefExtension_Wiki">
								<wikiExtraInfo Class="ProjectParagon.Wiki.WikiExtraInfo_WikiThingDef"/>
							</li>
						</modExtensions>
						<comps>
							<li Class="CompProperties_Rottable">
								<daysToRotStart>60</daysToRotStart>
								<rotDestroys>true</rotDestroys>
							</li>
							<li Class="ProjectParagon.Core.FoodSystem.CompProperties_FoodComponent">
								<slot>Medical</slot>
								<!-- Слот еды -->
								<foodSettings>
									<li>
										<foodTemplate>HotDrink</foodTemplate>
										<ingestible>
											<li>
												<joyKind>Chemical</joyKind>
												<joy>0.0</joy>
												<drugCategory>Social</drugCategory>
												<outcomeDoers>
													<li Class="IngestionOutcomeDoer_GiveHediff">
														<hediffDef>HerbalHigh</hediffDef>
														<severity>0.2</severity>
													</li>
												</outcomeDoers>
											</li>
										</ingestible>
										<foodParams>
											<li>
												<param>Nutrition</param>
												<baseValue>0.01</baseValue>
												<compatabilityWithOtherFood/>
											</li>
											<li>
												<param>Protein</param>
												<baseValue>0</baseValue>
												<compatabilityWithOtherFood/>
											</li>
											<li>
												<param>Fat</param>
												<baseValue>0</baseValue>
												<compatabilityWithOtherFood/>
											</li>
											<li>
												<param>Carbohydrates</param>
												<baseValue>0.005</baseValue>
												<compatabilityWithOtherFood/>
											</li>
											<li>
												<param>Vitamins</param>
												<baseValue>0.005</baseValue>
												<compatabilityWithOtherFood/>
											</li>
											<li>
												<param>Flavor</param>
												<baseValue>0.0</baseValue>
												<compatabilityWithOtherFood>
													<li>
														<def>Flora_RawCoffee</def>
														<value>-1</value>
													</li>
													<li>
														<def>Flora_RawRosehip</def>
														<value>0.7</value>
													</li>
													<li>
														<def>Milk</def>
														<value>-2</value>
													</li>
													<li>
														<def>Flora_RawCocoa</def>
														<value>-2</value>
													</li>
													<li>
														<def>Flora_RawMint</def>
														<value>0.8</value>
													</li>
													<li>
														<def>Ambrosia</def>
														<value>-2</value>
													</li>
													<li>
														<def>Flora_RawTea</def>
														<value>1.0</value>
													</li>
													<li>
														<def>PsychoidLeaves</def>
														<value>0.5</value>
													</li>
													<li>
														<def>Flora_RawPoppy</def>
														<value>0.5</value>
													</li>
													<li>
														<def>Flora_RawAloe</def>
														<value>0.8</value>
													</li>
												</compatabilityWithOtherFood>
											</li>
										</foodParams>
									</li>
								</foodSettings>
							</li>
						</comps>
					</ThingDef>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>*/ThingDef[defName="MedicineIndustrial"]</xpath>
				<value>
					<ThingDef ParentName="MedicineBase">
						<defName>MedicineIndustrial</defName>
						<label>medicine</label>
						<description>A kit of industrial-era medical equipment. It contains basic drugs, tools for suturing and bone setting, diagnostic devices, and various pads and fluids.</description>
						<graphicData>
							<texPath>Things/Item/Resource/Medicine/MedicineIndustrial</texPath>
							<graphicClass>Graphic_StackCount</graphicClass>
							<drawSize>0.85</drawSize>
						</graphicData>
						<stackLimit>50</stackLimit>
						<techLevel>Industrial</techLevel>
						<statBases>
							<MarketValue>18</MarketValue>
							<Mass>0.50</Mass>
							<Flammability>0.7</Flammability>
							<DeteriorationRate>2</DeteriorationRate>
							<WorkToMake>700</WorkToMake>
							<MedicalPotency>1</MedicalPotency>
							<MedicalQualityMax>1</MedicalQualityMax>
							<MedicalTendMult>1.25</MedicalTendMult>
						</statBases>
						<costList>
							<MedicineHerbal>1</MedicineHerbal>
							<Neutroamine>1</Neutroamine>
							<Cloth>5</Cloth>
						</costList>
						<recipeMaker>
							<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
							<workSkill>Intellectual</workSkill>
							<recipeUsers Inherit="false">
								<li>DrugLab</li>
							</recipeUsers>
							<researchPrerequisite>MedicineProduction</researchPrerequisite>
							<skillRequirements>
								<Crafting>4</Crafting>
								<Intellectual>4</Intellectual>
							</skillRequirements>
						</recipeMaker>
						<allowedArchonexusCount>10</allowedArchonexusCount>
					</ThingDef>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>*/ThingDef[defName="MedicineUltratech"]</xpath>
				<value>
					<ThingDef ParentName="MedicineBase">
						<defName>MedicineUltratech</defName>
						<label>glitterworld medicine</label>
						<description>A kit of advanced ultra-tech medical supplies, probably manufactured on a distant glitterworld. It contains advanced polymorphic drugs, nanite diagnostic and healing assisters, a mini-imager, and various multi-use tools.</description>
						<graphicData>
							<texPath>Things/Item/Resource/Medicine/MedicineUltratech</texPath>
							<graphicClass>Graphic_StackCount</graphicClass>
							<drawSize>0.85</drawSize>
						</graphicData>
						<stackLimit>50</stackLimit>
						<techLevel>Ultra</techLevel>
						<statBases>
							<MarketValue>50</MarketValue>
							<Mass>0.50</Mass>
							<Flammability>0.7</Flammability>
							<MedicalPotency>1.60</MedicalPotency>
							<MedicalQualityMax>1.30</MedicalQualityMax>
							<DeteriorationRate>2.0</DeteriorationRate>
							<MedicalTendMult>2</MedicalTendMult>
						</statBases>
						<tradeTags>
							<li>ExoticMisc</li>
						</tradeTags>
						<thingSetMakerTags>
							<li>RewardStandardHighFreq</li>
						</thingSetMakerTags>
						<allowedArchonexusCount>5</allowedArchonexusCount>
					</ThingDef>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>*/ThingDef[defName="Neutroamine"]</xpath>
				<value>
					<ThingDef ParentName="ResourceBase">
						<defName>Neutroamine</defName>
						<label>neutroamine</label>
						<description>A synthetic precursor chemical. While it is useless on its own, many drugs require neutroamine as an ingredient.</description>
						<possessionCount>10</possessionCount>
						<graphicData>
							<texPath>Things/Item/Resource/Neutroamine</texPath>
							<graphicClass>Graphic_StackCount</graphicClass>
							<drawSize>0.90</drawSize>
						</graphicData>
						<stackLimit>150</stackLimit>
						<healthAffectsPrice>false</healthAffectsPrice>
						<statBases>
							<MaxHitPoints>50</MaxHitPoints>
							<MarketValue>6</MarketValue>
							<Mass>0.02</Mass>
							<Flammability>0.7</Flammability>
							<DeteriorationRate>1.0</DeteriorationRate>
						</statBases>
						<thingCategories>
							<li>Chemistry</li>
						</thingCategories>
						<costList>
							<Chemfuel>1</Chemfuel>
							<Cyclinder_Ammonia>1</Cyclinder_Ammonia>
							<Glass>4</Glass>
						</costList>
						<recipeMaker>
							<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
							<workSkill>Intellectual</workSkill>
							<recipeUsers Inherit="false">
								<li>DrugLab</li>
							</recipeUsers>
							<researchPrerequisite>MedicineProduction</researchPrerequisite>
							<skillRequirements>
								<Crafting>4</Crafting>
								<Intellectual>4</Intellectual>
							</skillRequirements>
							<productCount>4</productCount>
						</recipeMaker>
					</ThingDef>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>