<Defs>
	<!---->
	<ThingDef ParentName="ApparelMakeableBase">
		<defName>Apparel_TacticalPolo</defName>
		<label>Polo shirt</label>
		<description>A garment that has a short fastening with a stand-up collar and short sleeves.</description>
		<graphicData>
			<texPath>Core/Items/Apparel/TacticalPolo/TacticalPolo</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<costStuffCount>50</costStuffCount>
		<techLevel>Industrial</techLevel>
		<recipeMaker>
			<researchPrerequisite>ComplexClothing</researchPrerequisite>
			<recipeUsers Inherit="false">
				<li>ElectricTailoringBench</li>
				<li>SteamLoom</li>
			</recipeUsers>
		</recipeMaker>
		<stuffCategories>
			<li>Fabric</li>
		</stuffCategories>
		<costList>
			<ComponentSimple>2</ComponentSimple>
		</costList>
		<thingCategories>
			<li>Torso</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>3000</WorkToMake>
			<Mass>0.25</Mass>
			<StuffEffectMultiplierArmor>1</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.3</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.2</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
			</bodyPartGroups>
			<wornGraphicPath>Core/Items/Apparel/TacticalPolo/TacticalPolo</wornGraphicPath>
			<layers>
				<li>OnSkin</li>
			</layers>
			<tags>
				<li>IndustrialBasic</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_StandardApparel"/>
	</ThingDef>
