	<ThingDef ParentName="HeatingElement">
		<defName>HeatingElement_Solid</defName>
		<label>Solid state heating element</label>
		<description>A separate solid fuel heating element that needs to be connected to the desired building.</description>
		<thingClass>ProjectParagon.Core.Things.Building_SolidFuelSource</thingClass>
		<graphicData>
			<texPath>Core/Buildings/SimpleScienceSet/SimpleScienceSet</texPath>
		</graphicData>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>150</costStuffCount>
		<costList>
			<StrongPlates>40</StrongPlates>
			<ComponentSimple>10</ComponentSimple>
		</costList>
		<designatorDropdown>Extensions_General</designatorDropdown>
		<researchPrerequisites>
			<li>Chemistry</li>
		</researchPrerequisites>
		<tickerType>Normal</tickerType>
		<comps>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>100.0</fuelConsumptionRate>
				<fuelCapacity>50.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>WoodLog</li>
						<li>Coal</li>
					</thingDefs>
				</fuelFilter>
				<initialFuelPercent>1</initialFuelPercent>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
				<consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
			</li>
		</comps>
	</ThingDef>
	
