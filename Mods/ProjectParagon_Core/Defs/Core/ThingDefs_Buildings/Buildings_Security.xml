			<texPath>Core/Buildings/Ballista/TurretBallista_Base</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2.3, 2.3)</drawSize>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
			<shadowData>
				<volume>(1.5,0.35,1.4)</volume>
				<offset>(0,0,-0.05)</offset>
			</shadowData>
		</graphicData>
		<statBases>
			<MaxHitPoints>350</MaxHitPoints>
			<WorkToBuild>25000</WorkToBuild>
			<Mass>250</Mass>
			<Bulk>250</Bulk>
		</statBases>
		<costList>
			<WoodLog>200</WoodLog>
			<StrongPlates>100</StrongPlates>
			<MechanismSimple>8</MechanismSimple>
			<ComponentSimple>12</ComponentSimple>
		</costList>
		<specialDisplayRadius>16</specialDisplayRadius>
		<building>
			<turretGunDef>DankPyon_Gun_BallistaTurret</turretGunDef>
			<ai_combatDangerous>true</ai_combatDangerous>
			<turretTopDrawSize>4</turretTopDrawSize>
		</building>
		<placeWorkers>
			<li>PlaceWorker_TurretTop</li>
			<li>PlaceWorker_ShowTurretRadius</li>
		</placeWorkers>
		<size>(3,3)</size>
		<interactionCellOffset>(-1,0,-1)</interactionCellOffset>
		<minifiedDef>MinifiedThing</minifiedDef>
		<researchPrerequisites>
			<li>AssaultWeapons</li>
		</researchPrerequisites>
		<designationCategory>Security</designationCategory>
		<techLevel>Medieval</techLevel>
		<designatorDropdown>MedievalSiegeWeapons</designatorDropdown>
	</ThingDef>
