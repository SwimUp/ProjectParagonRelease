			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>8000</WorkToBuild>
			<Flammability>0.7</Flammability>
		</statBases>
		<tickerType>Normal</tickerType>
		<size>(4,4)</size>
		<building>
			<destroySound>BuildingDestroyed_Metal_Big</destroySound>
		</building>
