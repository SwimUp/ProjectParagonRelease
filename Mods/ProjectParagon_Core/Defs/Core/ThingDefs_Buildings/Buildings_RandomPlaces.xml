<Defs>
	<ThingDef ParentName="CrateBase">
		<defName>Building_WoodenCrateI</defName>
		<label>wooden crate</label>
		<description>Small wooden box.</description>
		<graphicData>
			<texPath>Core/Buildings/RandomPlaces/Building_WoodenCrateIA</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>(1.3,1.3)</drawSize>
			<drawRotated>false</drawRotated>
			<color>(87, 58, 28)</color>
			<shadowData>
				<volume>(0.9, 2, 0.9)</volume>
				<offset>(0,0,0)</offset>
			</shadowData>
		</graphicData>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>100</costStuffCount>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints>
			<WorkToBuild>100</WorkToBuild>
			<Flammability>1</Flammability>
		</statBases>
		<size>(1,1)</size>
		<comps>
			<li Class="ProjectParagon.Core.Things.CompProperties_Container">
				<thingSetMakerDef>RaidLootMaker_Building_WoodenCrateI</thingSetMakerDef>
				<lootChance>0.15~0.3</lootChance>
			</li>
		</comps>
	</ThingDef>
