<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!---->
	<ThingDef ParentName="BuildingBase">
		<defName>SimpleSteamGenerator</defName>
		<label>Simple steam generator</label>
		<description>A simple design for turning water into steam. The resulting steam can be used in machines to generate electricity. Requires water containers and heaters to operate.</description>
		<thingClass>ProjectParagon.Core.Things.Building_SteamGenerator</thingClass>
		<graphicData>
			<texPath>Core/Buildings/SimpleSteamGenerator/SimpleSteamGenerator</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>5.9, 5.9</drawSize>
			<drawOffset>0, 0, 0</drawOffset>
			<shaderType>CutoutComplex</shaderType>
			<drawRotated>false</drawRotated>
		</graphicData>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>300</costStuffCount>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsPower</li>
		</thingCategories>
		<costList>
			<CinderBlock>100</CinderBlock>
			<ComponentSimple>10</ComponentSimple>
			<StrongPlates>50</StrongPlates>
			<MechanismSimple>12</MechanismSimple>
		</costList>
		<designatorDropdown>Power_SteamEngines</designatorDropdown>
		<researchPrerequisites>
			<li>SteamEnergyI</li>
		</researchPrerequisites>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<castEdgeShadows>false</castEdgeShadows>
		<canOverlapZones>true</canOverlapZones>
		<tickerType>Normal</tickerType>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>8000</WorkToBuild>
			<Mass>100</Mass>
			<Flammability>0</Flammability>
		</statBases>
		<size>(3,3)</size>
		<building>
			<destroySound>BuildingDestroyed_Metal_Small</destroySound>
		</building>
		<designationCategory>Power</designationCategory>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
		<placeWorkers>
			<li>ProjectParagon.Core.Things.PlaceWorker_ModularBuilding</li>
		</placeWorkers>
		<comps>
			<li Class="ProjectParagon.Core.Things.CompProperties_ModularBuilding">
				<scheme>SimpleSteamGenerator</scheme>
			</li>
			<li Class="ProjectParagon.Core.Things.CompProperties_SteamGenerator">
				<steamCapacity>200</steamCapacity>
				<maxGeneratedSteamPerIteration>5</maxGeneratedSteamPerIteration>
			</li>
		</comps>
		<staticSunShadowHeight>0.3</staticSunShadowHeight>
	</ThingDef>
