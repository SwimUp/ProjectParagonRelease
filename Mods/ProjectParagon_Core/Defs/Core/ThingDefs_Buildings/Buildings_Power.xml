	<ThingDef ParentName="BuildingBase">
		<defName>BioelectricCapsule</defName>
		<label>Bioelectric capsule</label>
		<description>Capsule for extracting energy from living beings. The larger the creature, the more energy can be extracted from it. Gradually kills the creature.</description>
		<thingClass>ProjectParagon.Core.Things.Building_BioelectricCapsule</thingClass>
		<graphicData>
			<texPath>Core/Buildings/BioelectricCapsule/BioelectricCapsule</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(4.2,4.2)</drawSize>
		</graphicData>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories inherit="false">
			<li>BuildingsSpecial</li>
		</thingCategories>
		<size>2,4</size>
		<altitudeLayer>Building</altitudeLayer>
		<rotatable>true</rotatable>
		<fillPercent>0.5</fillPercent>
