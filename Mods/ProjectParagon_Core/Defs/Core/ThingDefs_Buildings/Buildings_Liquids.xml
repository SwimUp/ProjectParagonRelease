<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>TightPipeline</defName>
		<label>Tight Pipeline</label>
		<description>Pipe for transporting various liquids.</description>
		<thingClass>Building</thingClass>
		<statBases>
			<MaxHitPoints>80</MaxHitPoints>
			<WorkToBuild>260</WorkToBuild>
			<Flammability>0.0</Flammability>
		</statBases>
		<graphicData Class="ProjectParagon.Core.Things.PipeGraphicData">
			<texPath>Rimefeller/Things/Building/oilPipe_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>Basic</linkType>
			<linkFlags>
				<li>Custom6</li>
			</linkFlags>
			<damageData>
				<rect>(0,0.35,1,0.3)</rect>
			</damageData>
		</graphicData>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>2</costStuffCount>
		<category>Building</category>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<selectable>true</selectable>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<repairEffect>Repair</repairEffect>
		<filthLeaving>Filth_RubbleBuilding</filthLeaving>
		<altitudeLayer>Conduits</altitudeLayer>
		<passability>Standable</passability>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<building>
			<blueprintGraphicData>
				<texPath>Rimefeller/Things/Building/oilPipe_Atlas_Blueprint</texPath>
			</blueprintGraphicData>
			<isInert>true</isInert>
			<isEdifice>false</isEdifice>
		</building>
		<rotatable>false</rotatable>
		<neverMultiSelect>true</neverMultiSelect>
		<clearBuildingArea>false</clearBuildingArea>
		<designationCategory>Production</designationCategory>
		<designatorDropdown>Miscellaneous</designatorDropdown>
		<placeWorkers>
			<li>ProjectParagon.Core.Things.PlaceWorker_Pipe</li>
		</placeWorkers>
		<tickerType>Never</tickerType>
		<drawerType>MapMeshOnly</drawerType>
		<uiIconPath>Rimefeller/UI/Plumbing</uiIconPath>
		<comps>
			<li Class="ProjectParagon.Core.Things.CompProperties_Pipe">
				<pipeTypeDef>Oil</pipeTypeDef>
				<canChangePipeType>true</canChangePipeType>
			</li>
		</comps>
		<researchPrerequisites>
			<li>Machining</li>
		</researchPrerequisites>
	</ThingDef>
	<ThingDef ParentName="BuildingBase">
		<defName>PipelineValve</defName>
		<label>pipeline valve</label>
		<description>Opens or closes connections between pipelines.</description>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Rimefeller/Things/Building/Valve</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1,1)</drawSize>
		</graphicData>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<isInert>true</isInert>
			<isEdifice>false</isEdifice>
			<blueprintGraphicData>
				<texPath>Rimefeller/Things/Building/oilPipe_Atlas_Blueprint</texPath>
			</blueprintGraphicData>
		</building>
		<statBases>
			<MaxHitPoints>80</MaxHitPoints>
			<WorkToBuild>350</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-2</Beauty>
			<Mass>2</Mass>
		</statBases>
		<hasInteractionCell>false</hasInteractionCell>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<neverMultiSelect>true</neverMultiSelect>
		<clearBuildingArea>false</clearBuildingArea>
		<placeWorkers>
			<li>ProjectParagon.Core.Things.PlaceWorker_Pipe</li>
		</placeWorkers>
		<altitudeLayer>Building</altitudeLayer>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>10</costStuffCount>
		<passability>Standable</passability>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<tickerType>Never</tickerType>
		<designationCategory>Production</designationCategory>
		<designatorDropdown>Miscellaneous</designatorDropdown>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<researchPrerequisites>
			<li>Machining</li>
		</researchPrerequisites>
		<pathCost>75</pathCost>
		<comps>
			<li Class="ProjectParagon.Core.Things.CompProperties_Pipe">
				<pipeTypeDef>Oil</pipeTypeDef>
				<canChangePipeType>true</canChangePipeType>
				<canBeValved>true</canBeValved>
			</li>
			<li Class="CompProperties_Flickable">
				<commandTexture>Rimefeller/Things/Building/Valve</commandTexture>
				<commandLabelKey>CommandDesignateOpenCloseValveLabelRimefeller</commandLabelKey>
				<commandDescKey>CommandDesignateOpenCloseValveDescRimefeller</commandDescKey>
			</li>
		</comps>
	</ThingDef>
