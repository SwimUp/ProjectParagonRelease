<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ProjectParagon.Core.Breeding.BreedingMethodDef>
		<defName>Hybridization</defName>
		<label>Hybridization</label>
		<description>The process of formation or production of hybrids, which is based on the combination of the genetic material of different cells in one cell.</description>
		<workTicks>200000</workTicks>
		<workerClass>ProjectParagon.Core.Breeding.Workers.BreedWorker_Hybridization</workerClass>
		<props Class="ProjectParagon.Core.Breeding.Workers.HybridizationProps">
			<seedsCost>5</seedsCost>
			<breedQualityByIntellectual>
				<points>
					<li>(0, 0.0000019)</li>
					<li>(5, 0.00001)</li>
					<li>(10, 0.0003)</li>
					<li>(15, 0.00033)</li>
					<li>(20, 0.0004)</li>
				</points>
			</breedQualityByIntellectual>
			<breedQualityRandomRangeMultByIntellectual>
				<points>
					<li>(0, 0.65)</li>
					<li>(5, 0.7)</li>
					<li>(10, 0.75)</li>
					<li>(15, 0.80)</li>
					<li>(20, 0.85)</li>
				</points>
			</breedQualityRandomRangeMultByIntellectual>
			<randomIterationsRange>5~10</randomIterationsRange>
			<randomValueRange>0.03~0.1</randomValueRange>
			<copySecondIngredientIngestiableCurveByQuality>
				<points>
					<li>(0.5, 0)</li>
					<li>(0.6, 0.05)</li>
					<li>(0.8, 0.4)</li>
					<li>(1, 0.7)</li>
				</points>
			</copySecondIngredientIngestiableCurveByQuality>
			<copySecondIngredientGlobalIngestiableCurveByQuality>
				<points>
					<li>(0.5, 0)</li>
					<li>(0.6, 0.05)</li>
					<li>(0.8, 0.2)</li>
					<li>(1, 0.4)</li>
				</points>
			</copySecondIngredientGlobalIngestiableCurveByQuality>
			<copySecondIngredientAlchemyIngredientCurveByQuality>
				<points>
					<li>(0.5, 0)</li>
					<li>(0.6, 0.02)</li>
					<li>(0.8, 0.25)</li>
					<li>(1, 0.65)</li>
				</points>
			</copySecondIngredientAlchemyIngredientCurveByQuality>
			<copySecondIngredientAlchemyIngredientsCountCurveByQuality>
				<points>
					<li>(0.5, 0)</li>
					<li>(0.6, 1)</li>
					<li>(0.8, 2)</li>
					<li>(1, 99999)</li>
				</points>
			</copySecondIngredientAlchemyIngredientsCountCurveByQuality>
		</props>
		<recipeLabelPrefix>Bring out the hybrid</recipeLabelPrefix>
		<breedLabelPrefix>hybrid</breedLabelPrefix>
	</ProjectParagon.Core.Breeding.BreedingMethodDef>

	<ProjectParagon.Core.Breeding.BreedingMethodDef>
		<defName>Mutational</defName>
		<label>Mutational</label>
		<description>The process of creating plants with improved characteristics by inducing mutations by physical or chemical factors.</description>
		<requiredResearch>MultiAnalyzer</requiredResearch>
	</ProjectParagon.Core.Breeding.BreedingMethodDef>

	<ProjectParagon.Core.Breeding.BreedingMethodDef>
		<defName>GeneticEngineering</defName>
		<label>Genetic engineering</label>
		<description>A set of techniques, methods and technologies for obtaining recombinant RNA and DNA, isolating genes from an organism (cells), manipulating genes, introducing them into other organisms and growing artificial organisms after removing selected genes from DNA.</description>
		<requiredResearch>MultiAnalyzer</requiredResearch>
	</ProjectParagon.Core.Breeding.BreedingMethodDef>

</Defs>
