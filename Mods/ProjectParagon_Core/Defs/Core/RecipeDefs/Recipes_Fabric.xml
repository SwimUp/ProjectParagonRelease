	<RecipeDef>
		<defName>Make_I_Cotton_Fabric</defName>
		<label>Make cotton fabric</label>
		<description>Make cotton fabric.</description>
		<jobString>Making cotton fabric.</jobString>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>1000</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Flora_RawCotton</li>
					</thingDefs>
				</filter>
				<count>30</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Flora_RawCotton</li>
			</thingDefs>
		</fixedIngredientFilter>
