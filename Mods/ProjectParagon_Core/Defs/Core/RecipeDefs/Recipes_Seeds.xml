<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<RecipeDef Name="MakeSeedParagonBase" Abstract="true">
		<description>Make plant seeds from its fruits.</description>
		<jobString>Making seeds.</jobString>
		<workAmount>600</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<requiredGiverWorkType>PlantCutting</requiredGiverWorkType>
		<effectWorking>Harvest_Plant</effectWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<workSkill>Plants</workSkill>
		<recipeUsers>
			<li>PlantProcessingTable</li>
		</recipeUsers>
	</RecipeDef>
	
	<RecipeDef ParentName="MakeSeedParagonBase">
		<defName>Make_MeatBushSeeds</defName>
		<label>make meat bush seeds</label>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RegularMeat</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>UraniumPowder</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<products>
			<SeedMeatBush>4</SeedMeatBush>
		</products>
		<researchPrerequisite>Agro_MutatedPlantsI</researchPrerequisite>
	</RecipeDef>
	
</Defs>
