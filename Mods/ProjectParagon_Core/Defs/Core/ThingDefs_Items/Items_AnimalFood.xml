<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="OrganicProductBase">
		<defName>MineralFeed</defName>
		<label>Mineral feed</label>
		<description>More balanced feed for feeding animals.</description>
		<thingCategories>
			<li>Foods</li>
		</thingCategories>
		<graphicData>
			<texPath>Core/Items/MineralFeed</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MarketValue>2</MarketValue>
			<Mass>0.015</Mass>
			<Nutrition>0.06</Nutrition>
			<FoodPoisonChanceFixedHuman>0.02</FoodPoisonChanceFixedHuman>
			<Protein>0.020</Protein>
			<Fat>0.020</Fat>
			<Carbohydrates>0.020</Carbohydrates>
			<Vitamins>0.005</Vitamins>
			<Flavor>-1.0</Flavor>
		</statBases>
		<ingestible>
			<foodType>Kibble</foodType>
			<preferability>RawBad</preferability>
			<tasteThought>AteKibble</tasteThought>
			<optimalityOffsetHumanlikes>-30</optimalityOffsetHumanlikes>
			<optimalityOffsetFeedingAnimals>20</optimalityOffsetFeedingAnimals>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>RawVegetable_Eat</ingestSound>
			<outcomeDoers>
				<li Class="ProjectParagon.Core.Hediffs.IngestionOutcomeDoers.IngestionOutcomeDoer_GiveHediffMaxSeverity">
					<hediffDef>QualityAnimalFood</hediffDef>
					<severityMax>1.99</severityMax>
					<severity>0.12</severity>
					<divideByBodySize>false</divideByBodySize>
					<animalOnly>true</animalOnly>
				</li>
			</outcomeDoers>
		</ingestible>
		<techLevel>Medieval</techLevel>
		<comps>
			<li Class="CompProperties_Ingredients">
				<performMergeCompatibilityChecks>false</performMergeCompatibilityChecks>
				<noIngredientsFoodKind>Meat</noIngredientsFoodKind>
			</li>
		</comps>
		<allowedArchonexusCount>200</allowedArchonexusCount>
	</ThingDef>
