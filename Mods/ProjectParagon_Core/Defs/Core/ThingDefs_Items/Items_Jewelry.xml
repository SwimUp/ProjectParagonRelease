<Defs>
	<ThingDef ParentName="ResourceBase">
		<defName>GoldRing</defName>
		<label>gold ring</label>
		<description>Small and elegant gold finger ring with a large gemstone. Always valued in the jewelry market.</description>
		<graphicData>
			<texPath>Core/Items/GoldRing/GoldRing</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<stackLimit>1</stackLimit>
		<healthAffectsPrice>true</healthAffectsPrice>
		<statBases>
			<MaxHitPoints>80</MaxHitPoints>
			<Mass>0.01</Mass>
			<Flammability>0</Flammability>
			<DeteriorationRate>0</DeteriorationRate>
			<WorkToMake>15000</WorkToMake>
		</statBases>
		<techLevel>Medieval</techLevel>
		<intricate>true</intricate>
		<thingCategories>
			<li>Jewerly</li>
		</thingCategories>
		<thingSetMakerTags>
			<li>RewardStandardHighFreq</li>
		</thingSetMakerTags>
		<stuffCategories>
			<li>Gems</li>
		</stuffCategories>
		<costStuffCount>1</costStuffCount>
		<costList>
			<GoldIngot>30</GoldIngot>
		</costList>
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Smith</effectWorking>
			<soundWorking>Recipe_Smith</soundWorking>
			<recipeUsers>
				<li>JewelryTable</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
			<researchPrerequisite>GemstoneProcessing</researchPrerequisite>
		</recipeMaker>
		<comps>
			<li>
				<compClass>CompQuality</compClass>
			</li>
			<li Class="CompProperties_Art">
				<nameMaker>NamerJewerly</nameMaker>
				<descriptionMaker>TalelessImages</descriptionMaker>
				<minQualityForArtistic>Awful</minQualityForArtistic>
			</li>
		</comps>
	</ThingDef>
