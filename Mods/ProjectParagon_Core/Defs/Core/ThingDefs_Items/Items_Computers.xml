	<ThingDef ParentName="ResourceBase">
		<defName>RAM_I</defName>
		<label>RAM (4GB)</label>
		<description>Random Access Memory device, a type of computer memory. An essential component for complex process calculations.</description>
		<graphicData>
			<texPath>Core/Items/Computers/RAM_I</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<stackLimit>1</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<techLevel>Industrial</techLevel>
		<statBases>
			<MaxHitPoints>15</MaxHitPoints>
			<MarketValue>100</MarketValue>
			<Mass>0.1</Mass>
			<Flammability>0.5</Flammability>
			<DeteriorationRate>4</DeteriorationRate>
		</statBases>
		<intricate>true</intricate>
		<thingCategories>
			<li>Electronics</li>
		</thingCategories>
		<thingSetMakerTags>
			<li>RewardStandardLowFreq</li>
		</thingSetMakerTags>
		<comps>
			<li Class="ProjectParagon.Core.Things.CompProperties_ComputerUnit">
				<ram>4</ram>
				<powerUsage>10</powerUsage>
			</li>
		</comps>
	</ThingDef>
