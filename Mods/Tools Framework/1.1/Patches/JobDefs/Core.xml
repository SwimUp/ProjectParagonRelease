			<!-- HaulToContainer -->
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/JobDef[defName="HaulToContainer"]</xpath>
				<value>
					<li Class="ToolsFramework.Job_Extension">
						<isPutAwayToolJob>true</isPutAwayToolJob>
					</li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>