<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
            <li>Vanilla Plants Expanded</li>
        </mods>
		<match Class="PatchOperationFindMod">
			<mods><li>VGP Vegetable Garden</li></mods>
			<match Class="PatchOperationSequence">
				<operations>
				<!--Change seed names-->
				<li Class="PatchOperationReplace">
					<xpath>/Defs/SeedsPlease.SeedDef[defName="Seed_VCEEggplant"]/label</xpath>
					<value><label>Black beauty aubergine seeds</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/SeedsPlease.SeedDef[defName="Seed_VCEPumpkin"]/label</xpath>
					<value><label>Mammoth gold pumpkin seeds</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/SeedsPlease.SeedDef[defName="Seed_VCETomato"]/label</xpath>
					<value><label>Red beefsteak tomato seeds</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/SeedsPlease.SeedDef[defName="Seed_VCEAppleTree"]/label</xpath>
					<value><label>Granny smith apple seeds</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/SeedsPlease.SeedDef[defName="Seed_VCEPeachTree"]/label</xpath>
					<value><label>Apricot seeds</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/SeedsPlease.SeedDef[defName="Seed_VCEBananaTree"]/label</xpath>
					<value><label>Gros michel banana seeds</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/SeedsPlease.SeedDef[defName="Seed_VCEOrangeTree"]/label</xpath>
					<value><label>Mandarin seeds</label></value>
				</li>
				
				<!--Change recipe names and description-->
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEEggplant"]/label</xpath>
					<value><label>Extract seeds from black beauty aubergine</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEEggplant"]/description</xpath>
					<value><description>Extract seeds from black beauty aubergine</description></value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEPumpkin"]/label</xpath>
					<value><label>Extract seeds from mammoth gold pumpkin</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEPumpkin"]/description</xpath>
					<value><description>Extract seeds from mammoth gold pumpkin</description></value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCETomato"]/label</xpath>
					<value><label>Extract seeds from red beefsteak tomato</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCETomato"]/description</xpath>
					<value><description>Extract seeds from red beefsteak tomato</description></value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEApple"]/label</xpath>
					<value><label>Extract seeds from granny smith apple</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEApple"]/description</xpath>
					<value><description>Extract seeds from granny smith apple</description></value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEPeach"]/label</xpath>
					<value><label>Extract seeds from apricot</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEPeach"]/description</xpath>
					<value><description>Extract seeds from apricot</description></value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEBanana"]/label</xpath>
					<value><label>Extract seeds from gros michel banana</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEBanana"]/description</xpath>
					<value><description>Extract seeds from gros michel banana</description></value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEOrange"]/label</xpath>
					<value><label>Extract seeds from mandarin</label></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="ExtractSeed_VCEOrange"]/description</xpath>
					<value><description>Extract seeds from mandarin</description></value>
				</li>
				</operations>
			</match>
			
			
		</match>
	</Operation>
</Patch>