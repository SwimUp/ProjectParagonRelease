<Defs>

	<RecipeDef ParentName="ExtractSeed">
	  <defName>ExtractSeed_VBE_Mint</defName>
	  <label>extract mint seeds</label>
	  <description>Extract seeds from mint leaves</description>
	  <ingredients>
		<li>
		  <filter>
			<thingDefs>
			  <li>VBE_MintLeaves</li>
			</thingDefs>
		  </filter>
		  <count>4</count>
		</li>
	  </ingredients>
	  <fixedIngredientFilter>
		<thingDefs>
		  <li>VBE_MintLeaves</li>
		</thingDefs>
	  </fixedIngredientFilter>
	  <products>
		<Seed_VBE_Mint>3</Seed_VBE_Mint>
	  </products>
	</RecipeDef>

	<RecipeDef ParentName="ExtractSeed">
