		<li>
		  <filter>
			<thingDefs>
			  <li>VCE_RawLemon</li>
			</thingDefs>
		  </filter>
		  <count>4</count>
		</li>
	  </ingredients>
	  <fixedIngredientFilter>
		<thingDefs>
		  <li>VCE_RawLemon</li>
		</thingDefs>
	  </fixedIngredientFilter>
	  <products>
		<Seed_VBE_Lemon>3</Seed_VBE_Lemon>
	  </products>
	</RecipeDef>

</Defs>