<Defs>
  <SeedsPlease.SeedDef ParentName="SeedBase">
    <defName>Seed_SyrCoffee</defName>
    <label>coffee seeds</label>
    <sources>
      <li>Plant_SyrCoffee</li>
    </sources>
    <graphicData><texPath>Things/Item/Seeds/Coffee</texPath></graphicData>
  </SeedsPlease.SeedDef>

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_SyrCoffee</defName>
    <label>extract coffee seeds</label>
    <description>Extract seeds from Coffee</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>SyrRawCoffee</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>SyrRawCoffee</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_SyrCoffee>1</Seed_SyrCoffee>
    </products>
  </RecipeDef>

  <SeedsPlease.SeedDef ParentName="SeedBase">
    <defName>Seed_SyrTea</defName>
    <label>tea seeds</label>
    <sources>
      <li>Plant_SyrTea</li>
    </sources>
    <graphicData><texPath>Things/Item/Seeds/Tea</texPath></graphicData>
  </SeedsPlease.SeedDef>

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_SyrTea</defName>
    <label>extract tea seeds</label>
    <description>Extract seeds from Tea</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>SyrRawTea</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>SyrRawTea</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_SyrTea>3</Seed_SyrTea>
    </products>
  </RecipeDef>
</Defs>
