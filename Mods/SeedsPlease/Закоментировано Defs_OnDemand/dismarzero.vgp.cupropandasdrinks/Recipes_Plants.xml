<Defs>
  <!-- Plants.xml -->

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Frassroot</defName>
    <label>extract frassroot seeds</label>
    <description>Extract seeds from FrassRoot.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>CPD_FrassRoot</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>CPD_FrassRoot</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Frassroot>3</Seed_Frassroot>
    </products>
  </RecipeDef>
  
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_CoffeeArabica</defName>
    <label>extract arabica coffee seeds</label>
    <description>Extract seeds from Arabica Coffee Beans.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>CPD_RawCoffeeBeans</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>CPD_RawCoffeeBeans</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_CoffeeArabica>3</Seed_CoffeeArabica>
    </products>
  </RecipeDef>

</Defs>
