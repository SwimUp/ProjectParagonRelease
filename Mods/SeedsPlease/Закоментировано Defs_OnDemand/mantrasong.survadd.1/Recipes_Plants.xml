 <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_SRV_Turnip</defName>
    <label>extract turnip seeds</label>
    <description>Extract seeds from turnip.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>SRV_TurnipItemSpawner</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>SRV_TurnipItemSpawner</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_SRV_Turnip>3</Seed_SRV_Turnip>
    </products>
  </RecipeDef>

