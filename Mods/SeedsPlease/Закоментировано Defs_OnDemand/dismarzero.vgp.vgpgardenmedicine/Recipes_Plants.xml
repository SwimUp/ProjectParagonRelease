<Defs>
  <!-- Cultivated_Plants_ambrosia.xml -->

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Ambrosia</defName>
    <label>extract ambrosia seeds</label>
    <description>Extract seeds from Ambrosia.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Ambrosia</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Ambrosia</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Ambrosia>3</Seed_Ambrosia>
    </products>
    <researchPrerequisite>VG_Ambrosia</researchPrerequisite>
  </RecipeDef>

  <!-- Cultivated_Plants.xml -->

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Neutroflower</defName>
    <label>extract neutroflower seeds</label>
    <description>Extract seeds from NeutroPetals.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>NeutroPetals</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>NeutroPetals</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Neutroflower>3</Seed_Neutroflower>
    </products>
    <researchPrerequisite>DrugProduction</researchPrerequisite>
  </RecipeDef>

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Tobacco</defName>
    <label>extract tobacco seeds</label>
    <description>Extract seeds from Tobacco.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawTobacco</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawTobacco</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Tobacco>3</Seed_Tobacco>
    </products>
  </RecipeDef>

</Defs>
