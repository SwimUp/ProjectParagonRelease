<Defs>
  <!-- ExpandedCropsPlantsCultivated.xml -->

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Tomato</defName>
    <label>extract tomato seeds</label>
    <description>Extract seeds from Tomatoes.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawTomatoes</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawTomatoes</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Tomato>3</Seed_Tomato>
    </products>
  </RecipeDef>

  <RecipeDef ParentName="ExtractSeed">
