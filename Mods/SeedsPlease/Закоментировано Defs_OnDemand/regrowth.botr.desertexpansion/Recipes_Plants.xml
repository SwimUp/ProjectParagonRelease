      <li>
        <filter>
          <thingDefs>
            <li>RawFungus</li>
          </thingDefs>
        </filter>
        <count>8</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawFungus</li>
      </thingDefs>
    </fixedIngredientFilter>
