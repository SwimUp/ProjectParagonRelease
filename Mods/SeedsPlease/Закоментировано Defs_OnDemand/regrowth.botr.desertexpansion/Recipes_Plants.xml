      <thingDefs>
        <li>RG_RawCactus</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_RG-D_AngelWingsCactus>3</Seed_RG-D_AngelWingsCactus>
    </products>
  </RecipeDef>

  <RecipeDef ParentName="ExtractSeed">
