    <label>Extract edible cactus seeds</label>
    <description>Extract seeds from raw cactus.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RG_RawCactus</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
