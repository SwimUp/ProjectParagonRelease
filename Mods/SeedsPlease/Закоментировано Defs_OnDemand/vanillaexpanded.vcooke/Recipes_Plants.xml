<Defs>

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_VCEAllspice</defName>
    <label>Extract allspice seeds</label>
    <description>Extract seeds from allspice.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>VCE_RawSpices</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>VCE_RawSpices</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_VCEAllspice>3</Seed_VCEAllspice>
    </products>
  </RecipeDef>
  
