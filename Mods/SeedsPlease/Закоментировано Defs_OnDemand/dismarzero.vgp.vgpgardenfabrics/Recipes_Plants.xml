<Defs>
  <!-- Cultivated_Plants.xml -->

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Flax</defName>
    <label>extract flax seeds</label>
    <description>Extract seeds from Flax.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawFlax</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawFlax</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Flax>3</Seed_Flax>
    </products>
  </RecipeDef>

</Defs>
