<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- Job -->
    <AutoPatcher.BeginDef>
        <defName>AP_BeginNode_JobDef</defName>
        <allDefs>true</allDefs>
        <defType>JobDef</defType>
    </AutoPatcher.BeginDef>

    <AutoPatcher.JobDriverInDefOut>
        <defName>AP_JobDriverInDefOut</defName>
    </AutoPatcher.JobDriverInDefOut>

    <AutoPatcher.JobDefInDriverOut>
        <defName>AP_JobDefInDriverOut</defName>
    </AutoPatcher.JobDefInDriverOut>

    <AutoPatcher.BeginDriver>
        <defName>AP_BeginNode_JobDriver</defName>
        <allDrivers>true</allDrivers>
        <baseDriver>JobDriver</baseDriver>
    </AutoPatcher.BeginDriver>

    <AutoPatcher.JobSearch>
        <defName>AP_JobSearch</defName>
        <findDeep>true</findDeep>
    </AutoPatcher.JobSearch>
    
    <!-- WorkGiver -->
    <AutoPatcher.BeginDef>
        <defName>AP_BeginNode_WorkGiverDef</defName>
        <allDefs>true</allDefs>
        <defType>WorkGiverDef</defType>
    </AutoPatcher.BeginDef>

    <AutoPatcher.WorkGiverInDefOut>
        <defName>AP_WorkGiverInDefOut</defName>
    </AutoPatcher.WorkGiverInDefOut>

    <AutoPatcher.WorkGiverDefInGiverOut>
        <defName>AP_WorkGiverDefInGiverOut</defName>
    </AutoPatcher.WorkGiverDefInGiverOut>

    <AutoPatcher.BeginDriver>
        <defName>AP_BeginNode_WorkGiver</defName>
        <allDrivers>true</allDrivers>
        <baseDriver>WorkGiver</baseDriver>
    </AutoPatcher.BeginDriver>

    <!-- Stat -->
    <AutoPatcher.BeginDef>
        <defName>AP_BeginNode_StatDef</defName>
        <allDefs>true</allDefs>
        <defType>StatDef</defType>
    </AutoPatcher.BeginDef>

    <AutoPatcher.StatWorkerInDefOut>
        <defName>AP_StatWorkerInDefOut</defName>
    </AutoPatcher.StatWorkerInDefOut>

    <AutoPatcher.StatDefInWorkerOut>
        <defName>AP_StatDefInWorkerOut</defName>
    </AutoPatcher.StatDefInWorkerOut>

    <AutoPatcher.BeginDriver>
        <defName>AP_BeginNode_StatWorker</defName>
        <allDrivers>true</allDrivers>
        <baseDriver>StatWorker</baseDriver>
    </AutoPatcher.BeginDriver>

    <AutoPatcher.BeginDef>
        <defName>AP_BeginNode_StatDef_WorkSpeed</defName>
        <allDefs>false</allDefs>
        <defType>StatDef</defType>
        <defListName>
            <li>MiningSpeed</li>
            <li>DeepDrillingSpeed</li>
            <li>SmoothingSpeed</li>
            <li>ResearchSpeed</li>
            <li>AnimalGatherSpeed</li>
            <li>PlantWorkSpeed</li>
            <li>ConstructionSpeed</li>
        </defListName>
    </AutoPatcher.BeginDef>

    <AutoPatcher.BeginDef>
        <defName>AP_BeginNode_StatDef_WorkYield</defName>
        <allDefs>false</allDefs>
        <defType>StatDef</defType>
        <defListName>
            <li>MiningYield</li>
            <li>AnimalGatherYield</li>
            <li>PlantHarvestYield</li>
        </defListName>
    </AutoPatcher.BeginDef>

    <AutoPatcher.BeginDef>
        <defName>AP_BeginNode_StatDef_WorkSuccess</defName>
        <allDefs>false</allDefs>
        <defType>StatDef</defType>
        <defListName>
            <li>ConstructSuccessChance</li>
            <li>FixBrokenDownBuildingSuccessChance</li>
        </defListName>
    </AutoPatcher.BeginDef>

    <AutoPatcher.StatSearch>
        <defName>AP_StatSearch</defName>
        <findDeep>true</findDeep>
        <findPawn>false</findPawn>
    </AutoPatcher.StatSearch>

    <AutoPatcher.StuffStatInject>
        <defName>AP_StuffStatInject</defName>
        <asFactor>true</asFactor>
    </AutoPatcher.StuffStatInject>

    <!-- Action and Toil -->
    <AutoPatcher.ActionSearch>
        <defName>AP_ActionSearch</defName>
        <findDeep>true</findDeep>
    </AutoPatcher.ActionSearch>

    <AutoPatcher.JobDriverSearchToil>
        <defName>AP_ToilSearch</defName>
    </AutoPatcher.JobDriverSearchToil>

    <!-- ThingDef -->
    <AutoPatcher.BeginDef>
        <defName>AP_BeginNode_ThingDef</defName>
        <allDefs>true</allDefs>
        <defType>ThingDef</defType>
    </AutoPatcher.BeginDef>
</Defs>