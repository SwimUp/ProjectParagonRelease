<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BasedRimatomicsMom">
		<defName>NuclearResearchBench</defName>
		<label>Rimatomics research bench</label>
		<description>Contains Rimatomics research projects. Requires power.</description>
		<thingClass>Rimatomics.Building_RimatomicsResearchBench</thingClass>
		<graphicData>
			<texPath>Rimatomics/Things/RimatomicsBuildings/Research</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(6,3)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<size>(5,2)</size>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>150</costStuffCount>
		<costList>
			<Microelectronics>2</Microelectronics>
			<ComponentIndustrial>4</ComponentIndustrial>
			<Plastic>100</Plastic>
			<SimplePlates>100</SimplePlates>
			<LeadIngot>50</LeadIngot>
		</costList>
		<canOverlapZones>false</canOverlapZones>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0.20</staticSunShadowHeight>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>150</pathCost>
		<tickerType>Normal</tickerType>
		<statBases>
			<MaxHitPoints>250</MaxHitPoints>
			<WorkToBuild>8000</WorkToBuild>
			<Flammability>1.0</Flammability>
			<ResearchSpeedFactor>1.0</ResearchSpeedFactor>
		</statBases>
		<inspectorTabs>
			<li>Rimatomics.ITab_RimatomicsResearch</li>
		</inspectorTabs>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>100</basePowerConsumption>
				<shortCircuitInRain>true</shortCircuitInRain>
			</li>
			<li Class="CompProperties_Breakdownable"/>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<li>WeaponsBench</li>
					<li>ResearchReactor</li>
					<li>MultiAnalyzer</li>
					<li>ScientificMonitor</li>
					<li>SimpleScienceSet</li>
					<li>AdvancedScienceSet</li>
				</linkableFacilities>
			</li>
			<li>
				<compClass>CompReportWorkSpeed</compClass>
			</li>
		</comps>
		<fillPercent>0.5</fillPercent>
		<interactionCellIcon>DiningChair</interactionCellIcon>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<hasInteractionCell>true</hasInteractionCell>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<surfaceType>Item</surfaceType>
		<designationHotKey>Misc4</designationHotKey>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
			<li>PlaceWorker_ReportWorkSpeedPenalties</li>
		</placeWorkers>
		<researchPrerequisites>
			<li>RimatomicsActivate</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>5</constructionSkillPrerequisite>
		<designatorDropdown>Atomic_ResearchBenches</designatorDropdown>
	</ThingDef>
