<Defs>
<!--===================================== 9MM HG =============================-->
<SoundDef>
<defName>TMCShot_Generic9mmHG</defName>  
 <context>MapOnly</context>   <eventNames />  
 <maxVoices>4</maxVoices>   <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
      <li Class="AudioGrain_Folder">
            <clipFolderPath>TMC_9MM_HG</clipFolderPath>
          </li>
        </grains> <volumeRange>
          <min>50</min>        
          <max>60</max>
        </volumeRange>      
        <pitchRange>
          <min>0.97</min>        
          <max>1.03</max>
        </pitchRange>      
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>
  
  
