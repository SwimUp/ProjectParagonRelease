	</ResearchProjectDef>

  <ResearchProjectDef ParentName="SurvivalResearch_Medieval">
    <defName>MedievalToolsResearch</defName>
    <label>Medieval tools</label>
    <description>Develop Medieval tools.</description>
    <baseCost>500</baseCost>
    <researchViewX>2</researchViewX>
    <researchViewY>1</researchViewY>
    <tags>
      <li>ClassicStart</li>
    </tags>
  </ResearchProjectDef>

	<ResearchProjectDef Name="SurvivalResearch_Industrial" Abstract="true">
		<tab>SurvivalTools_ResearchTab</tab>
		<techLevel>Industrial</techLevel>
    <prerequisites>
      <li>MedievalToolsResearch</li>
    </prerequisites>
	</ResearchProjectDef>

  <ResearchProjectDef ParentName="SurvivalResearch_Industrial">
    <defName>ChainsawResearch</defName>
    <label>Chain motion</label>
    <description>Develop an advanced wood cutting machine.</description>
    <baseCost>500</baseCost>
    <researchViewX>4</researchViewX>
    <researchViewY>1</researchViewY>
    <prerequisites>
      <li>BiofuelRefining</li>
      <li>MedievalToolsResearch</li>
    </prerequisites>
  </ResearchProjectDef>
  
  <ResearchProjectDef ParentName="SurvivalResearch_Industrial">
    <defName>GasCompressionResearch</defName>
    <label>Gas compression usage</label>
    <description>Find out ho to use compressed air to your advantage.</description>
    <baseCost>500</baseCost>
    <researchViewX>4</researchViewX>
    <researchViewY>2</researchViewY>
    <prerequisites>
      <li>Machining</li>
    </prerequisites>
  </ResearchProjectDef>
  
  <ResearchProjectDef ParentName="SurvivalResearch_Industrial">
    <defName>BuildingToolResearch</defName>
    <label>Directed combustion tool</label>
    <description>Create an advanced building tool that allows for use of various building techniques - from punching to incinerating.</description>
    <baseCost>500</baseCost>
    <researchViewX>4</researchViewX>
    <researchViewY>3</researchViewY>
    <prerequisites>
      <li>GasCompressionResearch</li>
      <li>BiofuelRefining</li>
    </prerequisites>
  </ResearchProjectDef>

