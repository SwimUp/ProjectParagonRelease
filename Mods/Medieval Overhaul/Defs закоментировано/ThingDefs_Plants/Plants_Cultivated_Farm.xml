<Defs>

<!--=========================== Basic Agriculture ==============================-->

  <ThingDef ParentName="PlantBase">
    <defName>DankPyon_Plant_Garlic</defName>
    <label>garlic plant</label>
    <description>Leafy green stalks, whos base consists of a segmented off-white bulb. Garlic makes for a good seasoning and ingredient for many meals, either minced and sauteed for use as an ingredient, or pulverized and used as a seasoning.</description>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Nutrition>0.15</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Plants/GarlicPlant</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible />
    <plant>
      <!--fertilityMin></fertilityMin-->
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <maxMeshCount>9</maxMeshCount>
	    <harvestedThingDef>DankPyon_RawGarlic</harvestedThingDef>
      <harvestYield>14</harvestYield>
      <sowResearchPrerequisites>
        <li>DankPyon_BasicAgriculture</li>
      </sowResearchPrerequisites>	  
      <sowTags>
        <li>Ground</li>
		    <li>Hydroponics</li>
      </sowTags>
      <topWindExposure>0.20</topWindExposure>
      <growDays>3</growDays>
	    <fertilitySensitivity>0.2</fertilitySensitivity>
      <visualSizeRange>0.2~0.4</visualSizeRange>
      <immatureGraphicPath>Plants_Immature/GarlicPlant</immatureGraphicPath>
      <wildOrder>2</wildOrder>
      <purpose>Food</purpose>
      <sowMinSkill>3</sowMinSkill>
    </plant>
  </ThingDef>


