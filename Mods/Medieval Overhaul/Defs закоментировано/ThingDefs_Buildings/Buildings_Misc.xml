<Defs>


<!--============================== Linkables ==============================-->
	<ThingDef ParentName="BuildingBase">
		<defName>DankPyon_ToolRack</defName>
		<label>Tool Rack</label>
		<description>A collection of smithing tools. Increases workspeed when placed near medieval workbenches.</description>
		<graphicData>
			<texPath>Misc/ToolRack</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>4</drawSize>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<thingClass>Building</thingClass>
		<uiIconScale>1</uiIconScale>
		<uiIconPath>Misc/ToolRack_MenuIcon</uiIconPath>
		<uiIconOffset>(0, 0)</uiIconOffset>
		<size>(2,1)</size>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<passability>Standable</passability>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>0.1</fillPercent>
		<minifiedDef>MinifiedThing</minifiedDef>
		<canOverlapZones>false</canOverlapZones>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<thingCategories>
			<li>BuildingsMisc</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<WorkToBuild>2000</WorkToBuild>
			<Mass>15</Mass>
			<Flammability>1.0</Flammability>
		</statBases>
		<costList>
			<DankPyon_IronIngot>140</DankPyon_IronIngot>
		</costList>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>60</costStuffCount>
		<comps>
			<li Class="CompProperties_Facility">
				<statOffsets>
					<WorkTableWorkSpeedFactor>0.06</WorkTableWorkSpeedFactor>
				</statOffsets>
				<maxDistance>6</maxDistance>
				<maxSimultaneous>3</maxSimultaneous>
			</li>
		</comps>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
		<building>
			<canPlaceOverWall>true</canPlaceOverWall>
			<isEdifice>false</isEdifice>
		</building>
		<clearBuildingArea>false</clearBuildingArea>
		<constructionSkillPrerequisite>4</constructionSkillPrerequisite>
		<designationCategory>Misc</designationCategory>
		<researchPrerequisites>
			<li>Smithing</li>
		</researchPrerequisites>
	</ThingDef>

