<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <RecipeDef Abstract="True" Name="DankPyon_MakeTanningLiquorBase">
		<jobString>Boiling wood into 6 tanning liquor.</jobString>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<workAmount>450</workAmount>
		<workSkill>Cooking</workSkill>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<workSkillLearnFactor>0.25</workSkillLearnFactor>
		<targetCountAdjustment>5</targetCountAdjustment>
		<products>
			<DankPyon_TanningLiquor>6</DankPyon_TanningLiquor>
		</products>
		<recipeUsers>
			<li>Campfire</li>
			<li>FueledStove</li>
			<li>ElectricStove</li>
			<li>DankPyon_RusticCookingTable</li>
		</recipeUsers>
		<researchPrerequisites>
			<li>DankPyon_LeatherTanning</li>
		</researchPrerequisites>
	</RecipeDef>

	<RecipeDef ParentName="DankPyon_MakeTanningLiquorBase">
		<defName>MakeTanningLiquorFromWood</defName>
		<label>make tanning liquor from wood</label>
		<description>Make a batch of tanning liquor using the bark and wood contents from logs.</description>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>WoodLog</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

	<RecipeDef ParentName="DankPyon_MakeTanningLiquorBase">
		<defName>DankPyon_MakeTanningLiquorFromFood</defName>
		<label>make tanning liquor from plants</label>
		<description>Make a batch of tanning liquor using any organic ingredients known to harbor tannins.</description>
		<jobString>Boiling plants into 6 tanning liquor.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RawHops</li>
						<li>RawAgave</li>
						<li>RawBerries</li>
						<li>RawCorn</li>
					</thingDefs>
				</filter>
				<count>28</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>RawHops</li>
				<li>RawAgave</li>
				<li>RawBerries</li>
				<li>RawCorn</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

