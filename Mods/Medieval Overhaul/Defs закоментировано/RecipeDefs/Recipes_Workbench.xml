<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Ammuniation: Base Class --> 
 <RecipeDef Name="DankPyon_MakeableAmmunitionBase" Abstract="True">
	<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<workSkill>Crafting</workSkill>
	<workSkillLearnFactor>0.25</workSkillLearnFactor>
	<effectWorking>Smith</effectWorking>
	<allowMixingIngredients>false</allowMixingIngredients>	
	<soundWorking>Recipe_Smith</soundWorking>
		<recipeUsers>
			<li>DankPyon_Workbench</li>
		</recipeUsers>	
</RecipeDef>	

<!-- Ammuniation: Ballista Bolts --> 


<RecipeDef ParentName="DankPyon_SmelterBase">
	<defName>DankPyon_MakeBullet_BallistaBolt</defName>
	<workAmount>200</workAmount>		
	<label>craft ballista bolts x10</label>
	<jobString>Crafting 10 ballista bolts.</jobString>
	<targetCountAdjustment>5</targetCountAdjustment>	
  <researchPrerequisites>
    <li>DankPyon_Ballista</li>
  </researchPrerequisites>
	<ingredients>
		<li>
			<filter>
				<thingDefs>
					<li>DankPyon_IronIngot</li>
				</thingDefs>
			</filter>
			<count>5</count>
		</li>
		<li>
			<filter>
				<thingDefs>
					<li>WoodLog</li>
				</thingDefs>
			</filter>
			<count>5</count>
		</li>
	</ingredients>
	<fixedIngredientFilter>
		<thingDefs>
			<li>DankPyon_IronIngot</li>
			<li>WoodLog</li>
		</thingDefs>
	</fixedIngredientFilter>
	<products>
		<DankPyon_BallistaBolt>10</DankPyon_BallistaBolt>
	</products>		
</RecipeDef>

