	<RecipeDef Abstract="True" Name="DankPyon_RugBase">
		<description>Craft a patched rug from different types of leather.</description>
		<jobString>Crafting a patched rug.</jobString>
		<workAmount>5000</workAmount>
		<targetCountAdjustment>1</targetCountAdjustment>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<workSkill>Crafting</workSkill>
		<effectWorking>Tailor</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<!--unfinishedThingDef>DankPyon_UnfinishedHide</unfinishedThingDef-->
		<recipeUsers>
			<li>DankPyon_TanningRack</li>
		</recipeUsers>
		<researchPrerequisites>
			<li>DankPyon_LeatherTanning</li>
		</researchPrerequisites>
	</RecipeDef>

	<RecipeDef ParentName="DankPyon_RugBase">
		<defName>DankPyon_MakeRugPatched1x1c</defName>
		<label>craft patched rug 1x1</label>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Leathers</li>
					</categories>
				</filter>
				<count>25</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Leathers</li>
			</categories>
		</fixedIngredientFilter>
		<products>
			<DankPyon_RugPatched1x1c>1</DankPyon_RugPatched1x1c>
		</products>
	</RecipeDef>

