		<equippedStatOffsets>
			<SocialImpact>0.15</SocialImpact>
		</equippedStatOffsets>
		<apparel>
			<hatRenderedFrontOfFace>true</hatRenderedFrontOfFace>
			<bodyPartGroups>
				<li>FullHead</li>
			</bodyPartGroups>
			<wornGraphicPath>Apparel/Headgear/ScaryTribalMask/ScaryTribalMask/ScaryTribalMask</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>Neolithic</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
	</ThingDef>
