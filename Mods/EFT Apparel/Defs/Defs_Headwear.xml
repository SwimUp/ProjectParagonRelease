<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--==============================================
	================== Balaclave =====================
	===============================================-->
	<ThingDef ParentName="JDSFrontierFaceMaskBase">
		<defName>JDSTarkov_Headwear_Balaclava</defName>
		<label>Balaclava</label>
		<description>"A balaclava, also known as a balaclava helmet or Bally or ski mask, is a form of cloth headgear designed to expose only part of the face, usually the eyes and mouth."</description>
		<graphicData>
			<texPath>Things/FaceMask/Balaclava/Balaclava_UI</texPath>
		</graphicData>
		<apparel>
			<wornGraphicPath>Things/FaceMask/Balaclava/Balaclava</wornGraphicPath>
			<tags>
				<li>EFT_HeadWears</li>
				<li>EFT_ScavsRaiderGear</li>
			</tags>
		</apparel>
		<statBases>
			<MarketValue>50</MarketValue>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>2500</WorkToMake>
			<Mass>0.5</Mass>
			<Bulk>1</Bulk>
            <WornBulk>1</WornBulk>
            <ArmorRating_Blunt>0.02</ArmorRating_Blunt>
            <ArmorRating_Sharp>0.02</ArmorRating_Sharp>
            <Insulation_Cold>2</Insulation_Cold>
            <Insulation_Heat>0.5</Insulation_Heat>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<stuffCategories>
			<li>Fabric</li>
		</stuffCategories>
		<costStuffCount>35</costStuffCount>
		<recipeMaker>
			<skillRequirements>
				<Crafting>4</Crafting>
			</skillRequirements>
			<researchPrerequisite>FlakArmor</researchPrerequisite>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>SteamLoom</li>
			</recipeUsers>
		</recipeMaker>
		<equippedStatOffsets>
		</equippedStatOffsets>
		<thingCategories>  
	</thingCategories>
	</ThingDef>
	
