	<!-- ==================== Recipes ========================== -->

	<RecipeDef ParentName="AmmoRecipeBase">
		<defName>MakeAmmo_FN57x28mm_FMJ</defName>
		<label>make FN 5.7x28mm (FMJ) cartridge x500</label>
		<description>Craft 500 FN 5.7x28mm (FMJ) cartridges.</description>
		<jobString>Making FN 5.7x28mm (FMJ) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Alloys_Strong</li>
						<li>Alloys_Superalloys</li>
					</categories>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>BrassIngot</li>
					</thingDefs>
				</filter>
				<count>22</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SmokelessGunpowder</li>
					</thingDefs>
				</filter>
				<count>45</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Alloys_Strong</li>
				<li>Alloys_Superalloys</li>
			</categories>
			<thingDefs>
				<li>SmokelessGunpowder</li>
				<li>BrassIngot</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_FN57x28mm_FMJ>150</Ammo_FN57x28mm_FMJ>
		</products>
		<workAmount>2400</workAmount>
		<recipeUsers Inherit="false">
			<li>WeaponWorkbench</li>
		</recipeUsers>
		<researchPrerequisite>GasOperation</researchPrerequisite>
	</RecipeDef>

