		<statBases>
			<Mass>0.008</Mass>
			<Bulk>0.01</Bulk>
		</statBases>
		<tradeTags>
			<li>CE_AutoEnableTrade</li>
			<li>CE_AutoEnableCrafting</li>
		</tradeTags>
		<thingCategories>
			<li>AmmoFN57x28mm</li>
		</thingCategories>
	</ThingDef>

