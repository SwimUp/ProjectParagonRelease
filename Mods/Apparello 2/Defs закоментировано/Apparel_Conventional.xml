<!-- APRONS -->



	<!--Doctor apron-->

  <ThingDef ParentName="ApparelloVanillaBase">
    <defName>Apparello_MedicalApron</defName>
    <label>surgeon's apron</label>
    <description>Lovely plastic apron for all your bloodspilling needs.</description>

    <graphicData>
      <texPath>Apparel/Display/SurgeonApron</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>

    <thingCategories>
      <li>Torso</li>
      <li>ApparelloMedicalSet</li>
    </thingCategories>

    <recipeMaker>
      <researchPrerequisite>ApparelloResearchWork</researchPrerequisite>
    </recipeMaker>

    <costList>
      <MedicineIndustrial>1</MedicineIndustrial>
      <Cloth>80</Cloth>
    </costList>

    <statBases>
	      <WorkToMake>4000</WorkToMake>
		      <Mass>0.10</Mass>
      <MarketValue>200</MarketValue>
      <StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
    </statBases>
    <equippedStatOffsets>
      <MedicalTendQuality>0.10</MedicalTendQuality>
      <MedicalSurgerySuccessChance>0.05</MedicalSurgerySuccessChance>
    </equippedStatOffsets>
		<generateCommonality>1</generateCommonality>
    <apparel>
			<careIfWornByCorpse>false</careIfWornByCorpse>
      <bodyPartGroups>
        <li>Torso</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/ApronSurgeon/Apron</wornGraphicPath>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
        <li>Doctor</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>20</weight>
					<only>RGBA(0.3,0.8,0.6,1)</only>
				</li>
			</options>
		</colorGenerator>
  </ThingDef>






