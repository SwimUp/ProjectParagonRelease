	<!-- Helmet -->
	<ThingDef ParentName="ApparelBase">
		<defName>CorvoMask</defName>
		<label>corvo's mask</label>
		<description>An advanced mask with gadgets that provides great protection.</description>
		<graphicData>
			<texPath>Armors/Mask/Mask</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1.5</drawSize>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<uiIconScale>1.4</uiIconScale>
		<costList>
			<Kevlar>5</Kevlar>
			<Synthread>10</Synthread>
			<Steel>70</Steel>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<techLevel>Industrial</techLevel>
		<tradeability>All</tradeability>
		<generateCommonality>0</generateCommonality>
		<generateAllowChance>0</generateAllowChance>
		<statBases>
			<WorkToMake>250000</WorkToMake> <!-- накидываем worktomake дабы повысить цену -->
			<MaxHitPoints>150</MaxHitPoints>
			<Mass>3.0</Mass>
			<Flammability>0.4</Flammability>
			<StuffEffectMultiplierArmor>3.5</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.70</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.60</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>2</EquipDelay>
			<Insulation_Cold>4</Insulation_Cold>
			<Insulation_Heat>2</Insulation_Heat>
			<Bulk>1.5</Bulk>
			<WornBulk>3</WornBulk>
			<MarketValue>800</MarketValue>
		</statBases>
		<costStuffCount>100</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
		</stuffCategories>
		<equippedStatOffsets>
			<PsychicSensitivity>-0.6</PsychicSensitivity>
			<SmokeSensitivity>-0.6</SmokeSensitivity>
			<ToxicEnvironmentResistance>0.20</ToxicEnvironmentResistance>
			<MedicalTendSpeed>0.25</MedicalTendSpeed>
			<MeleeHitChance>0.2</MeleeHitChance>
			<MeleeDodgeChance>0.1</MeleeDodgeChance>
			<ShootingAccuracyPawn>0.08</ShootingAccuracyPawn>
		</equippedStatOffsets>
		<thingCategories Inherit="False">
			<li>ArmorHeadgear</li>
		</thingCategories>
		<tradeTags>
			<li>Armor</li>
		</tradeTags>
		<apparel>
			<bodyPartGroups>
				<li>FullHead</li>
				<li>Eyes</li>
				<li>Teeth</li>
			</bodyPartGroups>
			<wornGraphicPath>Armors/Mask/Mask</wornGraphicPath>
			<hatRenderedFrontOfFace>true</hatRenderedFrontOfFace>
