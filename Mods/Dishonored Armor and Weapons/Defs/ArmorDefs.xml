<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Armor -->
	<ThingDef ParentName="ApparelBase">
		<defName>CorvoJacket</defName>
		<description>A large black coat made from a strong fabric. A great choice for an assassin as it offers protection, while not inhibiting movement.</description>
		<label>corvo's jacket</label>
		<graphicData>
			<texPath>Armors/Corvo/Corvo</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1.5</drawSize>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<costList>
			<Kevlar>25</Kevlar>
			<Synthread>25</Synthread>
			<Steel>100</Steel>
			<ComponentIndustrial>3</ComponentIndustrial>
		</costList>
		<thingCategories>
			<li>Torso</li>
		</thingCategories>
		<techLevel>Industrial</techLevel>
		<tradeability>All</tradeability>
		<generateCommonality>0</generateCommonality>
		<generateAllowChance>0</generateAllowChance>
		<statBases>
			<WorkToMake>250000</WorkToMake> <!-- накидываем worktomake дабы повысить цену -->
			<MaxHitPoints>300</MaxHitPoints>
			<Mass>3.0</Mass>
			<Flammability>0.4</Flammability>
			<StuffEffectMultiplierArmor>6</StuffEffectMultiplierArmor>
			<EquipDelay>5</EquipDelay>
			<Insulation_Cold>24</Insulation_Cold>
			<Insulation_Heat>12</Insulation_Heat>
			<Bulk>1.5</Bulk>
			<WornBulk>2</WornBulk>
		</statBases>
		<tradeTags>
			<li>Armor</li>
		</tradeTags>
		<costStuffCount>200</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
		</stuffCategories>
		<equippedStatOffsets>
			<PainShockThreshold>0.3</PainShockThreshold>
			<IncomingDamageFactor>-0.1</IncomingDamageFactor>
			<ImmunityGainSpeed>0.05</ImmunityGainSpeed>
			<NegotiationAbility>0.1</NegotiationAbility>
			<TradePriceImprovement>0.1</TradePriceImprovement>
			<SocialImpact>0.1</SocialImpact>
			<HuntingStealth>0.5</HuntingStealth>
			<MedicalTendSpeed>0.25</MedicalTendSpeed>
			<MeleeHitChance>0.3</MeleeHitChance>
			<MeleeDodgeChance>0.3</MeleeDodgeChance>
			<ShootingAccuracyPawn>0.05</ShootingAccuracyPawn>
			<ReloadSpeed>0.1</ReloadSpeed>
			<MoveSpeed>0.15</MoveSpeed>
			<CarryBulk>15</CarryBulk>
			<ToxicEnvironmentResistance>0.3</ToxicEnvironmentResistance>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<wornGraphicPath>Armors/Corvo/Corvo</wornGraphicPath>
			<layers>
				<li>Shell</li>
				<li>Webbing</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
		</apparel>
		<thingSetMakerTags>
			<li>RewardStandardMidFreq</li>
		</thingSetMakerTags>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(1,1,1)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>
