  <!-- SadjuukBannerBase: Base All Banners -->
  <ThingDef Abstract="True" Name="SadjuukBannerBase" ParentName="BuildingBase">
    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <constructEffect>ConstructMetal</constructEffect>
    <category>Building</category>
    <altitudeLayer>Blueprint</altitudeLayer>
    <blockWind>false</blockWind>
    <castEdgeShadows>false</castEdgeShadows>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsFurniture</li>
    </thingCategories>
    <!--<placeWorkers>
		<li>PlaceWorker_NotUnderRoof</li>
    </placeWorkers>-->
    <passability>PassThroughOnly</passability>
    <pathCost>60</pathCost>
    <!--<building>
		<isEdifice>true</isEdifice>
    </building>
	<clearBuildingArea>true</clearBuildingArea>-->
    <researchPrerequisites>
      <li>SadjuukColonization</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
  </ThingDef>



