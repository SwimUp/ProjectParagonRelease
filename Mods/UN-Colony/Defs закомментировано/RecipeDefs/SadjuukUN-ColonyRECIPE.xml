    <workAmount>6000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>SadjuukChemfuelBarrel</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
