	<!-- SadjuukChemfuelRecipeD : 1 corps => 10 Chemfuel -->
  <RecipeDef>
    <defName>SadjuukChemfuelRecipeD</defName>
    <label>Make 10 Chemfuel from corps</label>
    <description>An inefficient extraction process from organic corps. The use of human bodies for industrial purposes constitutes a war crime and serious crimes against humanity.</description>
    <jobString>Refining chemfuel from organic.</jobString>
    <effectWorking>Cremate</effectWorking>
    <soundWorking>Recipe_Cremate</soundWorking>
    <workAmount>1000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Corpses</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Corpses</li>
      </categories>
      <disallowedCategories>
        <li>CorpsesMechanoid</li>
      </disallowedCategories>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>CorpsesAnimal</li>
      </categories>
    </defaultIngredientFilter>
    <products>
      <Chemfuel>10</Chemfuel>
    </products>
  </RecipeDef>
