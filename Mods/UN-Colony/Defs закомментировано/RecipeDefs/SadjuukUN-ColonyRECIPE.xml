  <!-- SadjuukConcreteRecipeA : 1 StoneChunks => 20 Concrete -->
  <RecipeDef>
    <defName>SadjuukConcreteRecipeA</defName>
    <label>Make 20 Concrete from Chunks</label>
    <description>Produce Concrete Slabs.</description>
    <jobString>Producing Concrete Slabs.</jobString>
    <workAmount>1500</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cremate</effectWorking>
    <soundWorking>Recipe_Cremate</soundWorking>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>StoneChunks</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>StoneChunks</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <SadjuukConcrete>20</SadjuukConcrete>
    </products>
  </RecipeDef>
